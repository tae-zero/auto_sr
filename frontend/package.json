    "react-dom": "19.1.0",
    "typescript": "5.9.2",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@tailwindcss/postcss": "4.1.11",
    "@types/node": "20.19.9",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.11"
  },
  "engines": {
    "node": "20.x",
    "pnpm": ">=6.0.0"
